{
  "name": "nuxt-vue-query",
  "version": "0.0.6",
  "description": "Add type safe TanStack Vue Query to your Nuxt project.",
  "keywords": [
    "vue",
    "nuxt",
    "module",
    "vue-query",
    "tanstack"
  ],
  "author": {
    "name": "Özüm Eldoğan",
    "email": "ozum@ozum.net",
    "url": "https://www.ozum.net/"
  },
  "homepage": "",
  "bugs": "https://github.com/ozum/nuxt-vue-query/issues",
  "repository": "ozum/nuxt-vue-query",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "peerDependencies": {
    "@tanstack/vue-query": "^4"
  },
  "dependencies": {
    "@nuxt/kit": "^3.1.2",
    "defu": "^6.1.2",
    "knitwork": "^1.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.1.2",
    "@nuxt/test-utils": "^3.1.2",
    "changelogen": "^0.4.1",
    "eslint": "^8.33.0",
    "nuxt": "^3.2.0",
    "type-fest": "^3.5.7",
    "vitest": "^0.28.3"
  }
}
